{
  "name": "TTabs",
  "author": "LiangxiongLyu",
  "description": "Stash and restore your tab groups with ease! Go to https://github.com/lvliangxiong/ttabs for more information.",
  "version": "0.3.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Click to see TTabs Records",
    "default_icon": {
      "16": "/ttabs_16.png",
      "32": "/ttabs_32.png",
      "64": "/ttabs_64.png",
      "128": "/ttabs_128.png"
    }
  },
  "icons": {
    "16": "/ttabs_16.png",
    "32": "/ttabs_32.png",
    "64": "/ttabs_64.png",
    "128": "/ttabs_128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "sessions",
    "tabGroups",
    "activeTab",
    "notifications"
  ],
  "commands": {
    "Save Tab Groups": {
      "suggested_key": {
        "mac": "MacCtrl+S"
      },
      "description": "Save all alive tabs groups"
    },
    "_execute_action": {
      "suggested_key": {
        "mac": "MacCtrl+T"
      },
      "description": "Open popup page"
    }
  },
  "background": {
    "service_worker": "static/js/background.js"
  }
}
